<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tg.base.tb.mapper.TbSysParamManagerMapper">

    <resultMap id="BaseResultMap" type="com.tg.base.tb.entity.TbSysParamManager">
            <id property="tgParamId" column="tg_param_id" jdbcType="INTEGER"/>
            <result property="key" column="key_" jdbcType="VARCHAR"/>
            <result property="value" column="value" jdbcType="VARCHAR"/>
            <result property="remark" column="remark" jdbcType="VARCHAR"/>
            <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateUser" column="update_user" jdbcType="VARCHAR"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        tg_param_id,key_,value,
        remark,create_user,create_time,
        update_user,update_time
    </sql>
    <delete id="delByKey">
        delete from tb_sys_param_manager
        where 1 = 1
          and key_ = #{key}
    </delete>
    <select id="selectByKey" resultType="com.tg.base.tb.entity.TbSysParamManager">
        select tg_param_id,key_,value,
               remark,create_user,create_time,
               update_user,update_time
        from tb_sys_param_manager
        where 1 = 1
        and key_ = #{key}
    </select>
</mapper>
