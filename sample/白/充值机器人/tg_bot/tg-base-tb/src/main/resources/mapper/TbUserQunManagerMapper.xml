<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tg.base.tb.mapper.TbUserQunManagerMapper">

    <resultMap id="BaseResultMap" type="com.tg.base.tb.entity.TbUserQunManager">
            <id property="userQunId" column="user_qun_id" jdbcType="INTEGER"/>
            <result property="tgQunId" column="tg_qun_id" jdbcType="VARCHAR"/>
            <result property="tgUserId" column="tg_user_id" jdbcType="VARCHAR"/>
            <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateUser" column="update_user" jdbcType="VARCHAR"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        user_qun_id,tg_qun_id,tg_user_id
            ,create_user,
        create_time,update_user,update_time
    </sql>
    <delete id="deleteByUserTgIdAndQunId">
        DELETE
        FROM tb_user_qun_manager
        WHERE 1 = 1
          AND tg_qun_id = #{tgQunId}
          AND tg_user_id = #{tgUserId}
    </delete>
    <select id="queryOneByUserTgIdAndQunId" resultType="com.tg.base.tb.entity.TbUserQunManager">
        select
            user_qun_id,
            tg_qun_id,
            tg_user_id,
            create_user,
            create_time,
            update_user,
            update_time
        from tb_user_qun_manager
        where 1 = 1
          AND tg_qun_id = #{tgQunId}
          AND tg_user_id = #{tgUserId}
    </select>
</mapper>
